<launch>
	<arg name="veh"/>
	<node ns="$(arg veh)" pkg="apriltags" type="post_pros.py" name="apriltags_post_pros_node" output="screen">

		<!-- Setup parameters -->
		<param name="~map_file" value="$(find simcity)/maps/censi_map.yaml"/>
		<param name="~tiles_file" value="$(find simcity)/tiles/tiles.yaml"/>
	</node>
</launch>