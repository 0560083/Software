<launch>
	<arg name="veh"/>
	<arg name="config" default="baseline"/>
	<arg name="local" default="false" doc="true for running everything you possibly can on laptop. false for everything running on vehicle."/>
	<arg name="param_file_name" default="default" doc="Specify a param file. ex:megaman"/>
	<arg name="map_loc" doc="Physical room location of the duckietown map."/>
	
	<group ns="$(arg veh)">
	    <remap from="apriltags_global" to="apriltags/image_raw" /> 
		<node pkg="apriltags" type="april_tag_detector" name="apriltags" output="screen">
		</node>
        <node pkg="apriltags" type="AprilPostPros.py" name="AprilPostPros" output="screen">
		</node>
	</group>
</launch>
