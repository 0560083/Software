<launch>

  <arg name="robot_name" default="robot1"/>

  <!-- turtlebot_teleop_key already has its own built in velocity smoother -->
  <node pkg="duckietown_teleop" type="duckietown_teleop_key" name="duckietown_teleop_keyboard"  output="screen">
    <param name="scale_linear" value="0.5" type="double"/>
    <param name="scale_angular" value="1.5" type="double"/>
    <remap from="duckietown_teleop_keyboard/cmd_vel" to="/$(arg robot_name)/wheels_driver_node/wheels_cmd"/>   <!-- cmd_vel_mux/input/teleop"/-->
  </node>
</launch>
